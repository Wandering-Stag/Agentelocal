{
    "name": "agente-limpio",
    "displayName": "Agente Limpio",
    "description": "Un asistente de IA local",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:agente-limpio.openPanel"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "agente-limpio.openPanel",
                "title": "Agente Limpio: Abrir Panel"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "@types/node": "18.x",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "node-fetch": "^3.3.2"
    }
}